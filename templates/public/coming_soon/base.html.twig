<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>

    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet">
    {% block stylesheets %}
        {{ encore_entry_link_tags('coming_soon') }}
    {% endblock %}
    <title>{% block title %}{% endblock %}</title>

    <style>
        body {
            background-color: #f3f3f3;
        }
    </style>

</head>

<body>

<div class="page-wrapper">
    <div class="content-wrapper">
        {% block body %}{% endblock %}
    </div>
    <!--end content-wrapper-->
    <footer>
        <div class="social-icons animate">
            {% for s in misc.social_media %}
                <a target="_blank" href="{{ s.url }}"><i class="{{ s.icon }}"></i></a>
            {% endfor %}
        </div>
    </footer>
</div>
<!--end page-wrapper-->

{% block javascripts %}
    {{ encore_entry_script_tags('coming_soon') }}
{% endblock %}

</body>
