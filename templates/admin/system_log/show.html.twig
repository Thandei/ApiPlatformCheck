{% extends 'admin/base.html.twig' %}

 {% block title %}{{ "app.admin.crud.systemlog"|trans }} {{ "app.admin.crud.details"|trans }}{% endblock %}
 {% block bodyTitle %}{{ "app.admin.crud.systemlog"|trans }} {{ "app.admin.crud.details"|trans }}{% endblock %}

 {% block body %}
     <div class="row">
         <div class="col-md-12">
             <div class="card">
                 <div class="card-header">
                     <h5 class="card-title">
                         {{ "app.admin.crud.systemlog"|trans }}
                     </h5>
                 </div>
                 <div class="card-body">
                     <div class="table-responsive dt-responsive">
                         <table class="table table-bordered">
                             <tbody>
                             <tr>
                                 <th>{{ "app.admin.crud.id"|trans|caseupper }}</th>
                                 <td>{{ system_log.id }}</td>
                             </tr>
                             <tr>
                                 <th>{{ "app.admin.crud.priority"|trans|caseupper }}</th>
                                 <td>{{ system_log.priority }}</td>
                             </tr>
                             <tr>
                                 <th>{{ "app.admin.crud.createdat"|trans|caseupper }}</th>
                                 <td>{{ system_log.createdAt ? system_log.createdAt|date(shared.defaultDatetimeFormat) : '' }}</td>
                             </tr>
                             <tr>
                                 <th>{{ "app.admin.crud.content"|trans|caseupper }}</th>
                                 <td>{{ system_log.content }}</td>
                             </tr>
                             </tbody>
                         </table>

                     </div>

                 </div>
                 <div class="card-footer d-flex justify-content-between">
                     <a class="btn btn-secondary" href="{{ path('app_admin_system_log_index') }}">{{ "app.admin.crud.backtolist"|trans }}</a>
                     <a class="btn btn-primary" href="{{ path('app_admin_system_log_edit', {'id': system_log.id}) }}">{{ "app.admin.crud.edit"|trans }}</a>
                     {{ include('admin/system_log/_delete_form.html.twig')}}
                 </div>
             </div>
         </div>
     </div>
 {% endblock %}
