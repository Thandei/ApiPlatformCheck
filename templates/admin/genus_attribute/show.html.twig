{% extends 'admin/base.html.twig' %}

{% block title %}Genus Attribute Details{% endblock %}
{% block bodyTitle %}Genus Attribute Details{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <h5 class="card-title">Genus Attribute Details</h5>
                </div>
                <div class="card-body">
                    <table class="table table-bordered">
                        <tbody>
                        <tr>
                            <th>ID</th>
                            <td>{{ genus_attribute.id }}</td>
                        </tr>
                        <tr>
                            <th>Name</th>
                            <td>{{ genus_attribute.name }}</td>
                        </tr>
                        <tr>
                            <th>Unit</th>
                            <td>{{ genus_attribute.unit }}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
                <div class="card-footer d-flex justify-content-between">
                    <a class="btn btn-secondary" href="{{ path('app_admin_genus_attribute_index') }}">back to list</a>
                    <a class="btn btn-primary" href="{{ path('app_admin_genus_attribute_edit', {'id': genus_attribute.id}) }}">edit</a>
                    {{ include('admin/genus_attribute/_delete_form.html.twig') }}
                </div>
            </div>
        </div>
    </div>

{% endblock %}

